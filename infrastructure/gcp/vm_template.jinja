resources:
- name: rds-instance
  type: sqladmin.v1beta4.instance
  properties:
   backendType: SECOND_GEN
   connectionName: {{ env["deployment"] }}:us-central1:rds-instance
   databaseVersion: MYSQL_5_6
   gceZone: us-central1
   instanceType: CLOUD_SQL_INSTANCE
   kind: sql#instance
   project: {{ env["deployment"] }}
   settings:
    tier: db-n1-standard-1
    ipConfiguration:
     authorizedNetworks:
      - kind: sql#aclEntry
        value: 0.0.0.0/0
- name: rds-user
  type: sqladmin.v1beta4.user
  properties:
   kind: sql#user
   name: csyemaster
   password: csyepassword
   instance: {{ env["deployment"] }}:us-central1:rds-instance
   project: {{ env["deployment"] }}
   host: "%"
- name: rds-database
  type: sqladmin.v1beta4.database
  properties:
   kind: sql#database
   instance: {{ env["deployment"] }}:us-central1:rds-instance
   project: {{ env["deployment"] }}
   charset: utf8
   name: csye6225